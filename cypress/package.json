{
  "name": "cypress",
  "version": "1.0.0",
  "description": "E2E tests for MMDC Student Portal",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "test:curriculum": "cypress run --spec 'cypress/e2e/curriculum.cy.ts'",
    "test:courses": "cypress run --spec 'cypress/e2e/courses.cy.ts'",
    "test:programs": "cypress run --spec 'cypress/e2e/programs.cy.ts'",
    "test:pricing": "cypress run --spec 'cypress/e2e/pricing.cy.ts'",
    "test:enrollment": "cypress run --spec 'cypress/e2e/enrollment.cy.ts'",
    "test:billing": "cypress run --spec 'cypress/e2e/billing.cy.ts'",
    "test:lms": "cypress run --spec 'cypress/e2e/lms.cy.ts'",
    "test:appointment": "cypress run --spec 'cypress/e2e/appointment.cy.ts'",
    "test:login": "cypress run --spec 'cypress/e2e/login.cy.ts'",
    "test:users": "cypress run --spec 'cypress/e2e/user-managment.cy.ts'",
    "test:all": "cypress run"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "student-portal"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "cypress": "^14.5.4"
  }
}
